{
  "info": {
    "name": "FastAPI Personas CRUD",
    "description": "API para gestión de personas con búsqueda",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Health Check",
      "request": {
        "method": "GET",
        "header": [],
        "url": "http://127.0.0.1:8000/health"
      }
    },
    {
      "name": "Listar Personas",
      "request": {
        "method": "GET",
        "header": [],
        "url": "http://127.0.0.1:8000/personas"
      }
    },
    {
      "name": "Crear Persona",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"first_name\": \"Juan\",\n  \"last_name\": \"Pérez\",\n  \"email\": \"juan@example.com\",\n  \"birth_date\": \"1990-05-15\"\n}"
        },
        "url": "http://127.0.0.1:8000/personas"
      }
    },
    {
      "name": "Poblar Base de Datos",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"cantidad\": 50\n}"
        },
        "url": "http://127.0.0.1:8000/personas/poblar"
      }
    },
    {
      "name": "Obtener Persona por ID",
      "request": {
        "method": "GET",
        "header": [],
        "url": "http://127.0.0.1:8000/personas/1"
      }
    },
    {
      "name": "Eliminar Persona",
      "request": {
        "method": "DELETE",
        "header": [],
        "url": "http://127.0.0.1:8000/personas/1"
      }
    },
    {
      "name": "Borrar Todas las Personas (Reset)",
      "request": {
        "method": "DELETE",
        "header": [],
        "url": "http://127.0.0.1:8000/personas/reset"
      }
    },
    {
      "name": "Estadísticas por Dominio",
      "request": {
        "method": "GET",
        "header": [],
        "url": "http://127.0.0.1:8000/personas/estadisticas/dominios"
      }
    },
    {
      "name": "Estadísticas de Edad",
      "request": {
        "method": "GET",
        "header": [],
        "url": "http://127.0.0.1:8000/personas/estadisticas/edad"
      }
    },
    {
      "name": "Buscar Personas",
      "request": {
        "method": "GET",
        "header": [],
        "url": "http://127.0.0.1:8000/personas/buscar/lopez"
      }
    },
    {
      "name": "Buscar Personas con Paginación",
      "request": {
        "method": "GET",
        "header": [],
        "url": "http://127.0.0.1:8000/personas/buscar/a?skip=0&limit=20"
      }
    }
  ]
}
